{
  "name": "coding-interview-backend-level-3",
  "version": "1.0.0",
  "description": "Coding Interview Backend Level 3",
  "type": "commonjs",
  "main": "src/index.ts",
  "scripts": {
    "build": "swc src -d dist --strip-leading-paths",
    "test": "jest e2e",
    "test:unit": "jest src/__tests__",
    "docker:start": "docker-compose up --build",
    "docker:restart": "docker-compose restart",
    "migration:create": "npm run typeorm -- migration:create src/migrations/$npm_config_name",
    "migration:generate": "npm run typeorm -- migration:generate src/migrations/$npm_config_name --dataSource src/config/database.ts",
    "migration:run": "npm run typeorm -- migration:run --dataSource src/config/database.ts",
    "migration:revert": "npm run typeorm -- migration:revert --dataSource src/config/database.ts",
    "typeorm": "tsx ./node_modules/typeorm/cli.js"
  },
  "author": "El Dorado Dev Team",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.7.2",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "mocha": "^10.7.0",
    "nodemon": "^3.1.9",
    "tsx": "^4.19.3",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@hapi/hapi": "^21.4.0",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "^7.0.3",
    "hapi-swagger": "^17.3.2",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.21"
  }
}
